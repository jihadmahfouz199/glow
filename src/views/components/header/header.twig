                                <salla-search inline oval height="48"></salla-search>
                            </div>
                        </div>
                        <div class="cols bb-icons flex justify-center">
                            <div class="bb-flex-justify max-[575px]:flex max-[575px]:justify-between">
                                <div class="bb-header-buttons h-full flex justify-end items-center">
                                    <div class="bb-acc-drop relative">
                                        <div class="header-account flex items-center gap-3">
                                            {% if user.type == 'user' %}
                                            <div class="relative">
                                                <!-- زر المستخدم -->
                                                <button onclick="toggleUserMenu()"
                                                    class="flex items-center gap-2 group">
                                                    <img src="{{ user.avatar }}" alt="User Avatar"
                                                        class="w-8 h-8 rounded-full border border-gray-300" />
                                                    <span
                                                        class="text-sm font-medium text-gray-800 group-hover:text-primary">{{
                                                        user.first_name }}</span>
                                                    <i class="sicon-keyboard_arrow_down text-sm text-gray-600"></i>
                                                </button>

                                                <!-- القائمة المنسدلة -->
                                                <ul id="userDropdownMenu"
                                                    class="hidden absolute right-0 mt-2 w-64 bg-white border border-gray-100 rounded-xl shadow-lg z-50 text-sm text-gray-700 overflow-hidden">
                                                    <li>
                                                        <a href="/notifications"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-notifications text-primary"></i> الإشعارات
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/orders"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-bag text-primary"></i> الطلبات
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/orders?status=pending"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-wallet text-primary"></i> بانتظار الدفع
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/wishlist"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-heart text-primary"></i> الأمنيات
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/account"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-user text-primary"></i> حسابي
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/reviews"
                                                            class="flex items-center gap-2 px-4 py-3 hover:bg-gray-50 transition">
                                                            <i class="sicon-star text-primary"></i> التقييمات
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a href="/logout"
                                                            class="flex items-center gap-2 px-4 py-3 text-red-600 hover:bg-gray-50 transition">
                                                            <i class="sicon-logout"></i> تسجيل الخروج
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div>

                                            <script>
                                                function toggleUserMenu() {
                                                    const menu = document.getElementById('userDropdownMenu');
                                                    menu.classList.toggle('hidden');
                                                }

                                                document.addEventListener('click', function (event) {
                                                    const menu = document.getElementById('userDropdownMenu');
                                                    const button = event.target.closest('button');
                                                    const clickedInside = menu.contains(event.target) || (button && button.onclick?.toString().includes('toggleUserMenu'));
                                                    if (!clickedInside) {
                                                        menu.classList.add('hidden');
                                                    }
                                                });
                                            </script>





                                            {% else %}
                                            {# زر تسجيل الدخول - للزوار #}
                                            <button aria-label="Login" onclick="salla.event.dispatch('login::open')"
