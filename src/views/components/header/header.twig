{% set important_links = theme.settings.get('important_links') %}
<!-- Header -->

<header class="store-header bb-header relative z-[5] border-b-[1px] border-solid border-[#eee]">

    <div class="top-header bg-[#3d4750] py-[6px] max-[991px]:hidden">
        <div
            class="flex flex-wrap justify-between relative items-center mx-auto min-[1400px]:max-w-[1320px] min-[1200px]:max-w-[1140px] min-[992px]:max-w-[960px] min-[768px]:max-w-[720px] min-[576px]:max-w-[540px]">
            <div class="flex flex-wrap w-full">
                <div class="w-full px-[12px]">
                    <div class="inner-top-header flex justify-between items-center">

                        <div class="text-white flex">
                            <ul class="flex items-center">
                                {% if store.contacts.mobile %}
                                <li class="mr-[8px]">
                                    <a href="tel:{{ store.contacts.mobile }}" aria-label="mobile">
                                        <i class="sicon-phone"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.contacts.email %}
                                <li class="mr-[8px]">
                                    <a href="mailto:{{ store.contacts.email }}" aria-label="Email">
                                        <i class="sicon-mail"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.contacts.whatsapp %}
                                <li class="mr-[8px]">
                                    <a href="https://wa.me/{{ store.contacts.whatsapp }}" target="_blank"
                                        aria-label="Whatsapp">
                                        <i class="sicon-whatsapp"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.contacts.telegram %}
                                <li class="mr-[8px]">
                                    <a href="{{ store.contacts.telegram }}" target="_blank" aria-label="telegram">
                                        <i class="sicon-telegram"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.social.instagram %}
                                <li class="mr-[8px]">
                                    <a href="{{ store.social.instagram }}" target="_blank" aria-label="Instagram">
                                        <i class="sicon-instagram"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.social.twitter %}
                                <li class="mr-[8px]">
                                    <a href="{{ store.social.twitter }}" target="_blank" aria-label="Twitter">
                                        <i class="sicon-twitter"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.social.snapchat %}
                                <li class="mr-[8px]">
                                    <a href="{{ store.social.snapchat }}" target="_blank" aria-label="Snapchat">
                                        <i class="sicon-snapchat"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if store.social.facebook %}
                                <li class="mr-[8px]">
                                    <a href="{{ store.social.facebook }}" target="_blank" aria-label="Facebook">
                                        <i class="sicon-facebook"></i>
                                    </a>
                                </li>
                                {% endif %}
                            </ul>
                        </div>

                        <div class="flex ">

                            <div class="header-buttons flex items-center gap-2">
                                {# زر تغيير العملة مع العلم والسهم #}
                                {% if store.settings.currencies_enabled %}
                                <button type="button"
                                    onclick="salla.event.dispatch('localization::open', { tab: 'currencies' })"
                                    class="transition-all duration-300 ease-in-out font-Poppins text-[14px] text-[#fff] font-light leading-[28px] tracking-[0.03rem] flex items-center gap-1">
                                    <span>{{ currency.name }}</span>
                                    <i class="sicon-keyboard_arrow_down text-xs"></i>
                                </button>
                                {% endif %}

                                {# مودال سلة - لا تحذفه #}
                                <salla-localization-modal></salla-localization-modal>
                            </div>

                            <div class="cols px-[12px]">
                                {# زر تغيير اللغة مع العلم والسهم #}
                                {% if store.settings.is_multilingual %}
                                <button type="button"
                                    onclick="salla.event.dispatch('localization::open', { tab: 'languages' })"
                                    class="transition-all duration-300 ease-in-out font-Poppins text-[14px] text-[#fff] font-light leading-[28px] tracking-[0.03rem] flex items-center gap-1">
                                    <span
                                        class="flag iti__flag iti__{{ user.language.country_code }} rounded-sm w-5 h-4"></span>
                                    <span class="uppercase">{{ user.language.name }}</span>
                                    <i class=" sicon-keyboard_arrow_down text-xs"></i>
                                </button>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="bottom-header py-[20px] max-[991px]:py-[15px]">
        <div
            class="flex flex-wrap justify-between relative items-center mx-auto min-[1400px]:max-w-[1320px] min-[1200px]:max-w-[1140px] min-[992px]:max-w-[960px] min-[768px]:max-w-[720px] min-[576px]:max-w-[540px]">
